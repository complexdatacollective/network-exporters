{
  "name": "@codaco/network-exporters",
  "version": "2.0.0",
  "description": "Utility for exporting a network canvas network",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codaco/network-exporters.git"
  },
  "author": "Complex Data Collective",
  "bugs": {
    "url": "https://github.com/codaco/network-exporters/issues"
  },
  "homepage": "https://github.com/codaco/network-exporters#readme",
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/eslint-parser": "~7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/runtime": "^7.17.8",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "jest": "^26.0.1",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.1",
    "archiver": "^5.3.0",
    "async": "^3.2.0",
    "eventemitter3": "^4.0.7",
    "jssha": "^3.2.0",
    "jszip": "^3.9.0",
    "lodash": "^4.17.19",
    "sanitize-filename": "^1.6.3",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "electron": "^18.0.1",
    "electron-log": "^4.1.2",
    "react": "^16.13.1 || 17.0.0 || 18.0.0",
    "react-dom": "^16.13.1 || 17.0.0 || 18.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](www|docs|node_modules|scripts|test)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "/node_modules/(?!codaco/ui).+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@codaco/ui).+\\.js$"
    ]
  }
}
